<%= render 'users/user_header', user: @user %>

<div class="photos-list">
  <article class="user-profile">
    <header>
      <h1>Photos</h1>
    </header>
    <section>
      <% @user.photos.each do |photo| %>
        <div class="image-tile">
          <%= image_tag photo.photo_data.url(:tile) %>
        </div> <!-- image-tile -->
      <% end %>
    </section>
  </article>
</div> <!-- row -->
